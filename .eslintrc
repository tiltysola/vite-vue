{
  "plugins": ["simple-import-sort"],
  "extends": ["eslint-config-ali/typescript/vue"],
  "rules": {
    "simple-import-sort/imports": [
      "error",
      {
        "groups": [["^vue"], ["^"], ["^@"], ["^@/pages", "^@/components"], ["^\\."], ["^\\u0000"]]
      }
    ],
    "no-console": 0
  }
}
